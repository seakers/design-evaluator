{
  "family": "evaluator",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "evaluator",
      "image": "923405430231.dkr.ecr.us-east-2.amazonaws.com/design-evaluator:latest",
      "essential": true,
      "command": ["/app/evaluator/bin/evaluator"],
      "environment": [
        { "name": "REQUEST_MODE", "value": "CRISP-ATTRIBUTES" },
        { "name": "APOLLO_URL", "value": "https://daphne-services.selva-research.com:8080/v1/graphql" },
        { "name": "APOLLO_URL_WS", "value": "wss://daphne-services.selva-research.com:8080/v1/graphql" },
        { "name": "VASSAR_REQUEST_URL", "value": "https://sqs.us-east-2.amazonaws.com/923405430231/vassar_request" },
        { "name": "VASSAR_RESPONSE_URL", "value": "https://sqs.us-east-2.amazonaws.com/923405430231/vassar_response" },
        { "name": "DEPLOYMENT_TYPE", "value": "AWS" },
        { "name": "CLUSTER_ARN", "value": "arn:aws:ecs:us-east-2:923405430231:cluster/daphne-cluster" },
        { "name": "SERVICE_ARN", "value": "arn:aws:ecs:us-east-2:923405430231:service/daphne-cluster/evaluator-service" }
      ],
      "mountPoints": [
        {
            "sourceVolume": "vassar_resources",
            "containerPath": "/resources",
            "readOnly": false
        }
      ],
      "interactive": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": true,
          "awslogs-group": "daphne-evaluator",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "daphne"
        }
      }
    }
  ],
  "volumes": [
    {
      "name": "vassar_resources",
      "efsVolumeConfiguration": {
          "fileSystemId": "fs-d3977da8",
          "rootDirectory": "/VASSAR_resources",
          "transitEncryption": "ENABLED"
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "4096",
  "taskRoleArn": "arn:aws:iam::923405430231:role/DaphneEvaluatorTaskRole",
  "executionRoleArn": "arn:aws:iam::923405430231:role/ecsTaskExecutionRole"
}